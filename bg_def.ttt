#-----------------------------------------------------------------------------------------
# Kombination Hintergrund + Weg für Citytraffic
#-----------------------------------------------------------------------------------------
# Hintergrund in Groups

# Verschiedene Verkehrsmittel, unterschiedlicher BG und Way 
$DEF CBG=&IF(BGTYPE>=1,[$1#=&IF(BGTYPE>=3,$2),$3])

# "normerle" MM-Strecke für Citytraffic - Kombination mit CBG
$DEF CWX=&IF(BGTYPE>=1,$1)

# Verschiedene Verkehrsmittel, einheitlicher BG und unterschiedlicher Way 
$DEF EWX=&IF(BGTYPE>=1,[$1#=$2])
$DEF EBG=&IF(BGTYPE>=3,$1)

# Verschiedene Verkehrsmittel, einheitlicher BG und einheitlicher Way mit Flags
%*DEF(ETA(B,T)=ENV=&IF(BGTYPE>=3,%B),&IF(BGTYPE>=1,[WT=%T][WA=ENV_WAYA]))

# Weg + Hintergrund für ein Verkehrsmittel
%*DEF(GBG(B,C)=ENV=[BG#=@@] &IF(BGTYPE>=1,(&IF(BGTYPE>=3,%B),%C));)

#-------------------------------------------------------------------
#Hintergrund in Einzelzeilen
# Weg + Hintergrund für ein Verkehrsmittel
%*DEF(LBG(B,C)=AP=[BG=@@] &IF(BGTYPE>=1,(&IF(BGTYPE>=3,%B),%C));)
   
# Weg + Hintergrund BUS
%*DEF(ABG(B)=%LBG(%B,ENV_WAYA))   

